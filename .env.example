# Rabbit Service Configuration
# Copy this file to .env and modify the values as needed

# =============================================================================
# Environment
# =============================================================================
MOON_RABBIT_ENVIRONMENT=PROD
MOON_RABBIT_ENABLE_CLIENT_CONFIG=true
MOON_RABBIT_ENABLE_SWAGGER=true
MOON_RABBIT_ENABLE_METRICS=true
MOON_RABBIT_USE_DATABASE=false

# =============================================================================
# Server Configuration
# =============================================================================
MOON_RABBIT_SERVER_NAME=rabbit
MOON_RABBIT_SERVER_NAMESPACE=moon
MOON_RABBIT_USE_RANDOM_NODE_ID=false
MOON_RABBIT_METADATA_TAG=rabbit
MOON_RABBIT_METADATA_REPOSITORY=https://github.com/aide-family/rabbit
MOON_RABBIT_METADATA_AUTHOR=Aide Family
MOON_RABBIT_METADATA_EMAIL=aidecloud@163.com

# HTTP Server
MOON_RABBIT_HTTP_ADDRESS=0.0.0.0:8080
MOON_RABBIT_HTTP_NETWORK=tcp
MOON_RABBIT_HTTP_TIMEOUT=10s

# gRPC Server
MOON_RABBIT_GRPC_ADDRESS=0.0.0.0:9090
MOON_RABBIT_GRPC_NETWORK=tcp
MOON_RABBIT_GRPC_TIMEOUT=10s

# Job Server (EventBus)
MOON_RABBIT_JOB_ADDRESS=0.0.0.0:9091
MOON_RABBIT_JOB_NETWORK=tcp
MOON_RABBIT_JOB_TIMEOUT=10s
MOON_RABBIT_JOB_PROTOCOL=GRPC

# =============================================================================
# JWT Configuration
# =============================================================================
MOON_RABBIT_JWT_SECRET=xxx
MOON_RABBIT_JWT_EXPIRE=600s
MOON_RABBIT_JWT_ISSUER=rabbit

# =============================================================================
# Database Configuration (Main)
# =============================================================================
MOON_RABBIT_MAIN_USERNAME=root
MOON_RABBIT_MAIN_PASSWORD=123456
MOON_RABBIT_MAIN_HOST=localhost
MOON_RABBIT_MAIN_PORT=3306
MOON_RABBIT_MAIN_DATABASE=rabbit
MOON_RABBIT_MAIN_DEBUG=false
MOON_RABBIT_MAIN_USE_SYSTEM_LOGGER=true

# =============================================================================
# Job Core Configuration
# =============================================================================
MOON_RABBIT_JOB_CORE_WORKER_TOTAL=10
MOON_RABBIT_JOB_CORE_TIMEOUT=10s
MOON_RABBIT_JOB_CORE_BUFFER_SIZE=1000

# =============================================================================
# Registry Configuration
# =============================================================================
MOON_RABBIT_REGISTRY_TYPE=

# =============================================================================
# Cluster Configuration
# =============================================================================
MOON_RABBIT_CLUSTER_NAME=moon.rabbit
MOON_RABBIT_CLUSTER_ENDPOINTS=
MOON_RABBIT_CLUSTER_TIMEOUT=10s
MOON_RABBIT_CLUSTER_PROTOCOL=GRPC

# =============================================================================
# ETCD Configuration
# =============================================================================
MOON_RABBIT_ETCD_ENDPOINTS=127.0.0.1:2379
MOON_RABBIT_ETCD_USERNAME=
MOON_RABBIT_ETCD_PASSWORD=

# =============================================================================
# Kubernetes Configuration
# =============================================================================
MOON_RABBIT_KUBERNETES_KUBECONFIG=~/.kube/config

# =============================================================================
# Swagger Basic Auth Configuration
# =============================================================================
MOON_RABBIT_SWAGGER_BASIC_AUTH_ENABLED=true
MOON_RABBIT_SWAGGER_BASIC_AUTH_USERNAME=moon.rabbit
MOON_RABBIT_SWAGGER_BASIC_AUTH_PASSWORD=rabbit.swagger

# =============================================================================
# Metrics Basic Auth Configuration
# =============================================================================
MOON_RABBIT_METRICS_BASIC_AUTH_ENABLED=true
MOON_RABBIT_METRICS_BASIC_AUTH_USERNAME=moon.rabbit
MOON_RABBIT_METRICS_BASIC_AUTH_PASSWORD=rabbit.metrics

# =============================================================================
# Logging Configuration
# =============================================================================
MOON_RABBIT_CONFIG_PATHS=

# =============================================================================
# Message Log Path Configuration
# =============================================================================
MOON_RABBIT_MESSAGE_LOG_PATH=
